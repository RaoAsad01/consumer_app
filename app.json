{
  "expo": {
    "name": "Hexallo",
    "slug": "consumerapp",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asadahmad.consumerapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to automatically detect your country code for phone number input.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to automatically detect your country code for phone number input."
      },
      "newArchEnabled": true
    },
    "android": {
      "package": "com.asad_ahmad.consumerapp",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "minSdkVersion": 23,
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResources": true,
      "newArchEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "32253af1-066b-49fc-9971-d66fb0bb8c2b"
      }
    }
  }
}
